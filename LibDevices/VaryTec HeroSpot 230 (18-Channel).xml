  <functions>
    <!-- pan/tilt ch: 1-4 -->
    <position>
      <pan dmxchannel="0" finedmxchannel="1">
        <range range="540"/>
      </pan>
      <tilt dmxchannel="2" finedmxchannel="3">
        <range range="220"/>
      </tilt>
    </position>
	<!-- pan/tilt speed ch: 5 -->
	<const dmxchannel="4" name="Pan Tilt Speed" val="0"/>
	<!-- dimmer ch: 6 -->
    <dimmer dmxchannel="5"/>
