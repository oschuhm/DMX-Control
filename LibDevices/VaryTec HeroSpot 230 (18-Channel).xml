	<!-- prism ch: 14 | prism rotation ch: 15 -->
    <prism dmxchannel="13">
      <prismrotation dmxchannel="14">
        <step type="stop" mindmx="0" maxdmx="7"/>
        <range type="cw" mindmx="8" maxdmx="129" minval="0.0666" maxval="2.8"/>
        <range type="ccw" mindmx="134" maxdmx="255" minval="0.0666" maxval="2.8"/>
      </prismrotation>
      <step type="open" mindmx="0" maxdmx="19" caption="Excluded"/>
      <step type="prism" caption="Linear">
        <step for="prismrotation" mindmx="50" maxdmx="75"/>
      </step>
      <step type="prism" caption="Circular">
        <step for="prismindex" mindmx="76" maxdmx="105"/>
