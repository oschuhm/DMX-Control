	  </step>
	  <step type="gobo" val="USER_VARYTEC_HERO230_2_5.png" mindmx="48" maxdmx="57" caption="Gobo 5" >
	  	 <range handler="goboshake" mindmx="118" maxdmx="127" minval="1" maxval="10" />
