    </prism>
	<!-- show mode ch: 16 -->
	<const dmxchannel="15" name="Auto Show" val="0"/>
	<!-- pan-tilt auto ch: 17 -->
	<const dmxchannel="16" name="Auto PanTilt" val="0"/>
	<!-- reset ch: 18 -->
	<const dmxchannel="17" name="Reset" val="0"/>
